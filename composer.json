{
    "name": "infyomlabs/laravel-generator",
    "type": "library",
    "description": "InfyOm Laravel Generator",
    "keywords": [
        "laravel",
        "api",
        "model",
        "request",
        "migration",
        "model",
        "crud",
        "repository",
        "view",
        "test",
        "generator",
        "swagger"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Mitul Golakiya",
        "email": "me@mitul.me"
    }],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.6.0",
        "illuminate/console": "^11.6.0",
        "laracasts/flash": "^3.2.3",
        "spatie/laravel-html": "^3.9.0",
        "symfony/var-exporter": "^7.0.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.1.3",
        "mockery/mockery": "^1.6.11",
        "orchestra/testbench": "^9.0.4",
        "pestphp/pest": "3.x-dev",
        "pestphp/pest-plugin-laravel": "3.x-dev"
    },
    "autoload": {
        "psr-4": {
            "InfyOm\\Generator\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\InfyOm\\Generator\\InfyOmGeneratorServiceProvider"
            ]
        }
    },
    "funding": [{
        "type": "opencollective",
        "url": "https://opencollective.com/infyomlabs"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
